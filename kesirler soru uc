using System;
namespace ConsoleApp36
{
    class Program
    {
        static void Main(string[] args)
        {
            double sayi1, sayi2, sayi3, sayi4, sayi5;
            Console.Write("sayi 1 giriniz: ");
            sayi1 = Convert.ToDouble(Console.ReadLine());
            Console.Write("sayi 2 giriniz: ");
            sayi2 = Convert.ToDouble(Console.ReadLine());
            Console.Write("sayi 3 giriniz: ");
            sayi3 = Convert.ToDouble(Console.ReadLine());
            Console.Write("sayi 4 giriniz: ");
            sayi4 = Convert.ToDouble(Console.ReadLine());
            Console.Write("sayi 5 giriniz: ");
            sayi5 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("iki katlarÄ±");
            Console.WriteLine(sayi1 * 2);
            Console.WriteLine(sayi2 * 2);
            Console.WriteLine(sayi3 * 2);
            Console.WriteLine(sayi4 * 2);
            Console.WriteLine(sayi5 * 2);
            Console.WriteLine("bese bolumleri");
            Console.WriteLine(sayi1 / 5);
            Console.WriteLine(sayi2 / 5);
            Console.WriteLine(sayi3 / 5);
            Console.WriteLine(sayi4 / 5);
            Console.WriteLine(sayi5 / 5);
            Console.ReadKey();
        }
    }
}
